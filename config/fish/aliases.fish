# Vim is nvim
alias vim "nvim"

# Bundler
alias be "bundle exec"

# Git
alias ga "git add -A"
alias gcm "git commit -m"
alias gd "git diff"
alias gl "git log"
alias gs "git status"
alias grd "git rm (git ls-files --deleted)"

# Dir
alias lsg "ll | grep"

# Fish configs
alias falias "nvim ~/.config/fish/aliases.fish"
alias fconfig "nvim ~/.config/fish/config.fish"

# PS
alias psa "ps aux"
alias psg "ps aux | grep "
alias psr 'ps aux | grep ruby'

# Kill
alias ka9='killall -9'
alias k9='kill -9'

